page {
  padding: 0;
}
.header {
  padding: 26rpx 0;
}
.header .icon-image {
  width: 40rpx;
  height: 40rpx;
}
.red_theme .goods-seckill {
  background-image: linear-gradient(90deg, #F95F2F 0%, #FF2C3C 100%);
}
.orange_theme .goods-seckill {
  background-image: linear-gradient(90deg, #ffd200 0%, #f7971e 100%);
}
.pink_theme .goods-seckill {
  background-image: linear-gradient(90deg, #fd498f 0%, #fa444d 100%);
}
.gold_theme .goods-seckill {
  background-image: linear-gradient(90deg, #ebc389 0%, #e0a356 100%);
}
.blue_theme .goods-seckill {
  background-image: linear-gradient(90deg, #56ccf2 0%, #2f80ed 100%);
}
.green_theme .goods-seckill {
  background-image: linear-gradient(90deg, #3de650 0%, #2ec840 100%);
}
.goods-seckill .goods-list {
  margin: 0 24rpx;
}
.goods-seckill .goods-list .goods-item {
  padding: 30rpx 24rpx;
  margin-top: 20rpx;
  border-radius: 20rpx;
}
.goods-seckill .goods-list .goods-item .goods-info {
  flex: 1;
  width: 450rpx;
}
.goods-seckill .goods-list .goods-item .goods-info .sale-info {
  padding: 4rpx 16rpx;
  background-color: #ffe9eb;
}
.goods-seckill .goods-list .goods-item .goods-info .info-footer .btn {
  padding: 13rpx 45rpx;
  font-family: 'PingFang SC';
  font-weight: normal;
  font-size: 28rpx;
  line-height: 42rpx;
}
.red_theme .goods-seckill .goods-list .goods-item .goods-info .info-footer .btn {
  background-image: linear-gradient(90deg, #F95F2F 0%, #FF2C3C 100%);
}
.orange_theme .goods-seckill .goods-list .goods-item .goods-info .info-footer .btn {
  background-image: linear-gradient(90deg, #ffd200 0%, #f7971e 100%);
}
.pink_theme .goods-seckill .goods-list .goods-item .goods-info .info-footer .btn {
  background-image: linear-gradient(90deg, #fd498f 0%, #fa444d 100%);
}
.gold_theme .goods-seckill .goods-list .goods-item .goods-info .info-footer .btn {
  background-image: linear-gradient(90deg, #ebc389 0%, #e0a356 100%);
}
.blue_theme .goods-seckill .goods-list .goods-item .goods-info .info-footer .btn {
  background-image: linear-gradient(90deg, #56ccf2 0%, #2f80ed 100%);
}
.green_theme .goods-seckill .goods-list .goods-item .goods-info .info-footer .btn {
  background-image: linear-gradient(90deg, #3de650 0%, #2ec840 100%);
}
.fixed-contain {
  position: fixed;
  right: 20rpx;
  bottom: 240rpx;
  z-index: 9;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  font-size: 26rpx;
  font-weight: 500;
  border: 1px solid #E5E5E5;
  background-color: #ffffff;
}

