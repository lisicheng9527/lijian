page {
  background-color: #fff;
}
page .user-vip .header {
  width: 750rpx;
  height: 412rpx;
  background-image: url(data:image/png;base64,);
  padding-top: 30rpx;
  background-size: 100% 100%;
  height: 382rpx;
}
page .user-vip .header .user-level {
  border: 1px solid white;
  border-radius: 100rpx;
  padding: 4rpx 20rpx;
  line-height: 30rpx;
}
page .user-vip .header .user-text {
  line-height: 50rpx;
  font-weight: bold;
}
page .user-vip .content {
  margin-top: -200rpx;
}
page .user-vip .content .vip-card-item {
  border-radius: 20rpx;
  height: 320rpx;
  width: 600rpx;
  position: relative;
  background-size: 100% 100%;
}
page .user-vip .content .vip-card-item .grade {
  line-height: 36rpx;
  background-color: rgba(0, 0, 0, 0.3);
  border-bottom-right-radius: 20rpx;
  border-top-left-radius: 20rpx;
  height: 50rpx;
  padding: 0 28rpx;
}
page .user-vip .content .vip-card-item .vip-name {
  padding: 20rpx 30rpx 14rpx 30rpx;
  font-size: 46rpx;
  text-align: center;
  align-items: flex-end;
}
page .user-vip .content .vip-card-item .vip-centent {
  padding: 0 30rpx;
}
page .user-vip .content .vip-grade-rule {
  margin: 30rpx 40rpx;
}
page .user-vip .content .vip-grade-rule .title {
  height: 100rpx;
  background: #fff;
  padding: 29rpx 0 31rpx 0;
}
page .user-vip .content .vip-grade-rule .checkbox-item {
  height: 100rpx;
  background: #fff;
  padding: 29rpx 0 31rpx 0;
  font-family: "PingFang SC";
  font-weight: normal;
  font-size: 28rpx;
  letter-spacing: 0.02rpx;
  text-align: left;
  color: #333;
}
page .user-vip .content .vip-grade-rule .checkbox-item-border {
  border-bottom: 1rpx solid #E5E5E5;
}

