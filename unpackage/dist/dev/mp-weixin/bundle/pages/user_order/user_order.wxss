page {
  height: 100%;
  padding: 0;
}
.order-list {
  height: 100%;
  display: flex;
  flex-direction: column;
}
.order-list .order-item {
  flex: 1;
  min-height: 0;
}
.order-contain {
  padding: 0 20rpx;
}
.login {
  margin: 400rpx auto;
  padding: 10rpx 20rpx;
  color: white;
  width: 200rpx;
}
.red_theme .login {
  background-color: #ff2c3c;
}
.orange_theme .login {
  background-color: #f7971e;
}
.pink_theme .login {
  background-color: #fa444d;
}
.gold_theme .login {
  background-color: #e0a356;
}
.blue_theme .login {
  background-color: #2f80ed;
}
.green_theme .login {
  background-color: #2ec840;
}
.order {
  padding-left: 20rpx;
  margin-top: 20rpx;
  border-radius: 5px;
  background-color: #ffffff;
}
.order-header {
  display: flex;
  height: 80rpx;
  align-items: center;
  padding-right: 20rpx;
  border-bottom: 1px solid #E5E5E5;
}
.order-header .order-sn {
  font-size: 28rpx;
}
.order-header .order-status {
  margin-left: auto;
  font-size: 26rpx;
}
.red_theme .order-header .order-status--primary {
  color: #FF2C3C;
}
.orange_theme .order-header .order-status--primary {
  color: #f7971e;
}
.pink_theme .order-header .order-status--primary {
  color: #fa444d;
}
.gold_theme .order-header .order-status--primary {
  color: #e0a356;
}
.blue_theme .order-header .order-status--primary {
  color: #2f80ed;
}
.green_theme .order-header .order-status--primary {
  color: #2ec840;
}
.order-header .order-status--muted {
  color: #999999;
}
.order-main .order-desc {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding: 20rpx;
  font-size: 24rpx;
  color: #999999;
}
.order-footer {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  height: 100rpx;
  padding-right: 20rpx;
  border-top: 1px solid #E5E5E5;
}
.order-footer .order-button {
  box-sizing: border-box;
  padding: 10rpx 30rpx;
  border-radius: 60px;
  font-size: 28rpx;
}
.order-footer .order-button:nth-child(n + 2) {
  margin-left: 20rpx;
}
.order-footer .order-button--primary {
  color: #ffffff;
}
.red_theme .order-footer .order-button--primary {
  background-color: #ff2c3c;
}
.orange_theme .order-footer .order-button--primary {
  background-color: #f7971e;
}
.pink_theme .order-footer .order-button--primary {
  background-color: #fa444d;
}
.gold_theme .order-footer .order-button--primary {
  background-color: #e0a356;
}
.blue_theme .order-footer .order-button--primary {
  background-color: #2f80ed;
}
.green_theme .order-footer .order-button--primary {
  background-color: #2ec840;
}
.order-footer .order-button--normal {
  border-width: 1px;
  border-style: solid;
}
.red_theme .order-footer .order-button--normal {
  border-color: #FF2C3C;
}
.orange_theme .order-footer .order-button--normal {
  border-color: #f7971e;
}
.pink_theme .order-footer .order-button--normal {
  border-color: #fa444d;
}
.gold_theme .order-footer .order-button--normal {
  border-color: #e0a356;
}
.blue_theme .order-footer .order-button--normal {
  border-color: #2f80ed;
}
.green_theme .order-footer .order-button--normal {
  border-color: #2ec840;
}
.red_theme .order-footer .order-button--normal {
  color: #FF2C3C;
}
.orange_theme .order-footer .order-button--normal {
  color: #f7971e;
}
.pink_theme .order-footer .order-button--normal {
  color: #fa444d;
}
.gold_theme .order-footer .order-button--normal {
  color: #e0a356;
}
.blue_theme .order-footer .order-button--normal {
  color: #2f80ed;
}
.green_theme .order-footer .order-button--normal {
  color: #2ec840;
}
.order-footer .order-button--muted {
  border: 1px solid #E5E5E5;
  color: #666666;
}
.order-tag {
  padding: 4rpx 10rpx;
  margin-right: 10rpx;
  border-radius: 2px;
  font-size: 22rpx;
  color: #ffffff;
}
.order-tag--pink {
  background: linear-gradient(#ff369a 0%, #ff2c3c 100%);
}
.order-tag--red {
  background: linear-gradient(#f95f2f 0%, #ff2c3c 100%);
}
.order-tag--blue {
  background: linear-gradient(#fa6aa9 0%, #fc00bb 100%);
}
.order-tag--green {
  background: linear-gradient(#0cc21e 0%, #06c160 100%);
}
.order-tag--purple {
  background: linear-gradient(#a411d1 0%, #a411d1 100%);
}
.goods-name {
  font-size: 28rpx;
  color: #333333;
  overflow: hidden;
}

