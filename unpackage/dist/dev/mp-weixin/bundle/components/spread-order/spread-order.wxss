.spread-order .order-lists {
  padding: 0 20rpx;
}
.spread-order .order-lists .order-item {
  border-radius: 14rpx;
}
.spread-order .order-lists .order-item .order-header {
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #E5E5E5;
}
.spread-order .order-lists .order-item .order-header .status {
  color: #F95F2F;
}
.spread-order .order-lists .order-item .order-content {
  padding: 20rpx 30rpx 20rpx 20rpx;
  border-bottom: 1px solid #E5E5E5;
}
.spread-order .order-lists .order-item .order-content .order-goods-info {
  text-align: left;
  flex: 1;
}
.spread-order .order-lists .order-item .order-content .order-goods-info .name {
  line-height: 36rpx;
}
.spread-order .order-lists .order-item .order-content .order-goods-info .pre-income {
  margin-top: 8rpx;
}
.spread-order .order-lists .order-item .order-footer {
  padding: 20rpx 30rpx 20rpx 20rpx;
}

