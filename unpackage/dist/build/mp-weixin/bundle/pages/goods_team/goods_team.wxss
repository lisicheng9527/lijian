.goods-team{display:flex;flex-direction:column;height:100vh;max-height:100vh;overflow:hidden}.goods-team .banner{display:flex;align-items:center;justify-content:center;min-height:100rpx;width:100%;margin-bottom:20rpx}.goods-team .banner .title-image{width:440rpx;height:48rpx}.goods-team .goods-contain{padding:0 20rpx}.goods-team .fixed-contain{position:fixed;right:20rpx;bottom:240rpx;z-index:9;display:flex;flex-direction:column;align-items:center;justify-content:center;width:110rpx;height:110rpx;border-radius:50%;font-size:26rpx;font-weight:500;border:1px solid #e5e5e5;background-color:#fff}.red_theme .bg--primary{background-image:linear-gradient(90deg,#f95f2f,#ff2c3c)}.orange_theme .bg--primary{background-image:linear-gradient(90deg,#ffd200,#f7971e)}.pink_theme .bg--primary{background-image:linear-gradient(90deg,#fd498f,#fa444d)}.gold_theme .bg--primary{background-image:linear-gradient(90deg,#ebc389,#e0a356)}.blue_theme .bg--primary{background-image:linear-gradient(90deg,#56ccf2,#2f80ed)}.green_theme .bg--primary{background-image:linear-gradient(90deg,#3de650,#2ec840)}.goods{margin-bottom:20rpx}.goods-button{padding:10rpx 35rpx;border-radius:60px;font-size:28rpx;color:#fff}.red_theme .goods-button{background-color:#ff2c3c}.orange_theme .goods-button{background-color:#f7971e}.pink_theme .goods-button{background-color:#fa444d}.gold_theme .goods-button{background-color:#e0a356}.blue_theme .goods-button{background-color:#2f80ed}.green_theme .goods-button{background-color:#2ec840}